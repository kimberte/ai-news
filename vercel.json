{
  "version": 2,
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "functions": {
    "app/api/cron/fetch-news/route.ts": {
      "runtime": "nodejs18.x",
      "maxDuration": 10,
      "schedule": "0 * * * *"
    }
  },
  "env": {
    "SUPABASE_URL": "@supabase_url",
    "SUPABASE_KEY": "@supabase_key",
    "NEWS_API_KEY": "@news_api_key"
  }
}
